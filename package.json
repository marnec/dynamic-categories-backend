{
  "name": "dynamic-categories",
  "version": "0.1.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "npm run typeorm:run && nest start --watch",
    "start:debug": "npm run typeorm:run && nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"./src/**/*.{tsx,ts}\"",
    "lint-fix": "eslint \"./src/**/*.{tsx,ts}\" --fix",
    "test": "jest --forceExit --detectOpenHandles --coverage",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "typeorm:run": "npm run typeorm migration:run -- -d ./src/ormconfig.ts --transaction all",
    "typeorm:generate": "npm run typeorm -- -d ./src/ormconfig.ts migration:generate ./src/migrations/$npm_config_name",
    "typeorm:create": "npm run typeorm -- migration:create ./src/migrations/$npm_config_name",
    "typeorm:revert": "npm run typeorm -- -d ./src/ormconfig.ts migration:revert",
    "sonar": "sonarqube-verify"
  },
  "dependencies": {
    "@nestjs/axios": "3.0.1",
    "@nestjs/common": "10.3.1",
    "@nestjs/config": "3.1.1",
    "@nestjs/core": "10.3.1",
    "@nestjs/platform-express": "10.3.1",
    "@nestjs/schedule": "4.0.0",
    "@nestjs/swagger": "7.2.0",
    "@nestjs/typeorm": "10.0.1",
    "@nestjs/passport": "10.0.3",
    "bcryptjs": "2.4.3",
    "fast-case": "1.7.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cron": "3.1.6",
    "passport": "0.7.0",
    "radash": "11.0.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "7.8.1",
    "swagger-ui-express": "5.0.0",
    "typeorm": "0.3.20",
    "typeorm-transactional": "0.5.0",
    "pg": "8.11.3",
    "uuid": "9.0.0",
    "luxon": "3.4.4",
    "zod": "3.22.4",
    "fast_array_intersect": "1.1.0",
    "@faker-js/faker": "8.4.1"
  },
  "devDependencies": {
    "@nestjs/cli": "10.3.0",
    "@nestjs/schematics": "10.1.0",
    "@nestjs/testing": "10.3.1",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.12",
    "@types/node": "20.11.16",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "6.20.0",
    "@typescript-eslint/parser": "6.20.0",
    "@types/multer": "1.4.11",
    "@types/luxon": "3.4.2",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-sonarjs": "0.23.0",
    "jest": "29.7.0",
    "jest-sonar-reporter": "^2.0.0",
    "prettier": "3.2.4",
    "prettier-eslint": "16.3.0",
    "sonarqube-scanner": "^3.0.1",
    "sonarqube-verify": "^1.0.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "5.1.3"
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-reporter.xml",
    "indent": 4
  }
}